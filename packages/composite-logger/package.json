{
  "name": "@unic/composite-logger",
  "version": "0.0.0",
  "description": "Lightweight logger composite to help you debug your modules and disable when you go to production",
  "scripts": {
    "start": "npm run dev",
    "dev": "ava --watch",
    "build": "webpack",
    "test": "ava"
  },
  "main": "dist/logger.js",
  "keywords": ["composite", "logger"],
  "author": "Unic AG",
  "repository": "https://github.com/unic/javascript-packages/tree/master/packages",
  "license": "Apache-2.0",
  "devDependencies": {
    "ava": "0.21.0",
    "babel-cli": "6.24.1",
    "babel-loader": "7.1.2",
    "babel-plugin-external-helpers": "6.22.0",
    "babel-preset-env": "1.6.1",
    "mock-local-storage": "1.0.5",
    "sinon": "4.1.3",
    "webpack": "3.10.0"
  },
  "ava": {
    "files": ["src/**/*.spec.js"],
    "source": ["src/**/*.js"],
    "concurrency": 5,
    "failFast": true,
    "powerAssert": true,
    "require": ["babel-register"],
    "babel": "inherit"
  }
}
