{
  "name": "@unic/pubsub",
  "version": "0.0.0",
  "description": "PubSub implemented as a singleton",
  "scripts": {
    "start": "npm run dev",
    "dev": "lerna exec --scope=@unic/pubsub -- ava --watch",
    "build": "lerna exec --scope=@unic/pubsub -- webpack --mode production --module-bind js=babel-loader"
  },
  "main": "dist/main.js",
  "module": "src/index.js",
  "keywords": [
    "singleton",
    "pubsub"
  ],
  "author": "Unic AG",
  "repository": "https://github.com/unic/javascript-packages/tree/master/packages/pubsub",
  "license": "Apache-2.0",
  "publishConfig": {
    "access": "public"
  },
  "ava": {
    "files": [
      "src/**/*.spec.js"
    ],
    "sources": [
      "src/**/*.js"
    ],
    "concurrency": 5,
    "failFast": true,
    "powerAssert": true,
    "require": [
      "babel-register"
    ]
  },
  "dependencies": {
    "@unic/composite-observer": "^0.0.10"
  }
}
