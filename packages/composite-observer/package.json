{
  "name": "@unic/composite-observer",
  "version": "0.0.8",
  "description": "Simple observer composite to use for your factories",
  "scripts": {
    "start": "npm run dev",
    "dev": "ava --watch",
    "build": "webpack",
    "test": "ava"
  },
  "main": "dist/observer.js",
  "keywords": [
    "composite",
    "observer"
  ],
  "author": "Unic AG",
  "repository": "https://github.com/unic/javascript-packages/tree/master/packages/composite-observer",
  "license": "Apache-2.0",
  "devDependencies": {
    "babel-cli": "6.24.1",
    "babel-loader": "7.1.2",
    "babel-plugin-external-helpers": "6.22.0",
    "babel-preset-env": "1.6.1",
    "webpack": "3.10.0"
  },
  "ava": {
    "files": [
      "src/**/*.spec.js"
    ],
    "source": [
      "src/**/*.js"
    ],
    "concurrency": 5,
    "failFast": true,
    "powerAssert": true,
    "require": [
      "babel-register"
    ],
    "babel": "inherit"
  }
}
